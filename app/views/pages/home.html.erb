<div class="jumbotron fill">
  <h1><%= image_tag "logo.png", class: "img-fluid" %></h1>
    <div class="btn-group-vertical mt-4">
    <% if user_signed_in? %>
      <%= link_to "Let's Party!", new_event_path, class: "button rounded-decide" %>
      <%= link_to "My Events", events_path, class: "button rounded-decide" %>
      <%= link_to "Log Out", destroy_user_session_path, class: "button rounded-decide", method: :delete %>
    <br>
    <% else %>
      <%= link_to "SIGN UP", new_user_registration_path, class: "button rounded-decide" %>
      <%= link_to "LOG IN", new_user_session_path, class: "button rounded-decide" %>
    <% end %>
  </div>
</div>
